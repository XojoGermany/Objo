#tag WindowCode
	#tag Event
		Sub Open()
		  SourceCode.SmartDashes = False
		  SourceCode.SmartQuotes = False
		  
		  Self.Tokeniser = New ObjoTokeniser
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Sub DisplayTokens()
		  ListboxTokens.RemoveAllRows
		  
		  For Each token As ObjoToken In Tokens
		    
		    ListboxTokens.AddRow("")
		    
		    // Store the row.
		    Var row As Integer = ListboxTokens.LastAddedRowIndex
		    
		    // Line.
		    ListboxTokens.CellValueAt(row, COLUMN_LINE) = token.LineNumber.ToString
		    
		    // Token start position.
		    ListboxTokens.CellValueAt(row, COLUMN_START) = token.StartPosition.ToString
		    
		    // Token type.
		    ListboxTokens.CellValueAt(row, COLUMN_TYPE) = token.Type.ToString
		    
		    // Token lexeme.
		    ListboxTokens.CellValueAt(row, COLUMN_LEXEME) = token.Lexeme
		    
		  Next token
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		Tokeniser As ObjoTokeniser
	#tag EndProperty

	#tag Property, Flags = &h0
		Tokens() As ObjoToken
	#tag EndProperty


	#tag Constant, Name = COLUMN_LEXEME, Type = Double, Dynamic = False, Default = \"3", Scope = Private
	#tag EndConstant

	#tag Constant, Name = COLUMN_LINE, Type = Double, Dynamic = False, Default = \"0", Scope = Private
	#tag EndConstant

	#tag Constant, Name = COLUMN_START, Type = Double, Dynamic = False, Default = \"1", Scope = Private
	#tag EndConstant

	#tag Constant, Name = COLUMN_TYPE, Type = Double, Dynamic = False, Default = \"2", Scope = Private
	#tag EndConstant


#tag EndWindowCode

#tag Events ButtonTokenise
	#tag Event
		Sub Action()
		  Tokeniser.Reset
		  
		  Try
		    Tokens = Tokeniser.Tokenise(SourceCode.Text)
		    DisplayTokens
		    Info.Text = "Tokenised successfully."
		  Catch e As ObjoTokeniserException
		    Info.Text = "[" + e.LineNumber.ToString + ", " + _
		    e.LineCharacterPosition.ToString + "]: " + e.Message
		    ListboxTokens.RemoveAllRows
		  End Try
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ButtonClear
	#tag Event
		Sub Action()
		  SourceCode.Text = ""
		  ListboxTokens.RemoveAllRows
		  
		End Sub
	#tag EndEvent
#tag EndEvents
